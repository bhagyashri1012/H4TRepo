<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"              xmlns:app="http://schemas.android.com/apk/res-auto"              android:background="@color/white_pure"              android:orientation="vertical"              android:layout_width="match_parent" android:layout_height="match_parent">    <include layout="@layout/common_toolbar"/>    <ScrollView android:layout_width="match_parent"                android:layout_height="wrap_content"    >        <LinearLayout android:layout_width="match_parent"                      android:layout_height="wrap_content"                      android:layout_margin="@dimen/activity_vertical_margin"                      android:paddingBottom="100dp"                      android:orientation="vertical">            <TextView style="@style/CustomRegularBoldTextView"                      android:text="@string/price_text"/>            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="@dimen/activity_vertical_margin_half"                    android:layout_marginBottom="@dimen/margin_4dp"                    android:clipChildren="false">                <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                        android:id="@+id/range_seekbar_price"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        app:corner_radius="10"                        app:right_thumb_color="@color/colorPrimary"                        app:left_thumb_color="@color/colorPrimary"                        app:bar_color="@color/cool_grey"                        app:bar_highlight_color="@color/colorPrimary"                        app:min_value="0"                        app:max_value="1000000"                />                <TextView                        android:id="@+id/textMin_price"                        style="@style/CustomMediumTextView"                        android:text="0"                        android:layout_below="@id/range_seekbar_price"                        android:layout_marginTop="@dimen/margin_4dp"                        android:layout_marginStart="10dp"                />                <TextView                        android:id="@+id/textMax_price"                        style="@style/CustomMediumTextView"                        android:text="1000000"                        android:layout_below="@id/range_seekbar_price"                        android:layout_marginTop="@dimen/margin_4dp"                        android:layout_marginEnd="10dp"                        android:layout_alignParentRight="true"/>            </RelativeLayout>            <LinearLayout android:layout_width="wrap_content"                          android:layout_height="wrap_content"                          android:orientation="vertical"            android:visibility="gone"            >            <TextView style="@style/CustomRegularBoldTextView"                      android:text="@string/duration_text"/>            <TextView style="@style/CustomRegularTextView"                      android:layout_marginTop="@dimen/activity_horizontal_margin"                      android:text="OutboundTrip"/>            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="@dimen/activity_vertical_margin_half"                    android:layout_marginBottom="@dimen/margin_4dp"                    android:clipChildren="false">                <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                        android:id="@+id/range_seekbar_outbound_trip"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        app:right_thumb_color="@color/colorPrimary"                        app:left_thumb_color="@color/colorPrimary"                        app:bar_color="@color/cool_grey"                        app:bar_highlight_color="@color/colorPrimary"                        app:min_value="0"                        app:max_value="100"                />                <TextView                        android:id="@+id/textMin_outbound"                        style="@style/CustomMediumTextView"                        android:text="0"                        android:layout_below="@id/range_seekbar_outbound_trip"                        android:layout_marginTop="@dimen/margin_4dp"                        android:layout_marginStart="10dp"                />                <TextView                        android:id="@+id/textMax_outbound"                        android:text="Any"                        style="@style/CustomMediumTextView"                        android:layout_below="@id/range_seekbar_outbound_trip"                        android:layout_marginTop="@dimen/margin_4dp"                        android:layout_marginEnd="10dp"                        android:layout_alignParentRight="true"/>            </RelativeLayout>            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:orientation="vertical"                          android:id="@+id/ll_inbound"            >                <TextView style="@style/CustomRegularTextView"                          android:text="InboundTrip" android:layout_height="wrap_content"/>                <RelativeLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="@dimen/activity_vertical_margin_half"                        android:layout_marginBottom="@dimen/margin_4dp"                        android:clipChildren="false">                    <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                            android:id="@+id/range_seekbar_duration"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            app:right_thumb_color="@color/colorPrimary"                            app:left_thumb_color="@color/colorPrimary"                            app:bar_color="@color/cool_grey"                            app:bar_highlight_color="@color/colorPrimary"                            app:min_value="0"                            app:max_value="100"                    />                    <TextView                            android:id="@+id/textMin"                            style="@style/CustomMediumTextView"                            android:text="0"                            android:layout_below="@id/range_seekbar_duration"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginStart="10dp"                    />                    <TextView                            android:id="@+id/textMax"                            android:text="Any"                            style="@style/CustomMediumTextView"                            android:layout_below="@id/range_seekbar_duration"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginBottom="@dimen/margin_4dp"                            android:layout_marginEnd="10dp"                            android:layout_alignParentRight="true"/>                </RelativeLayout>            </LinearLayout>            </LinearLayout>            <TextView                    style="@style/CustomRegularBoldTextView"                    android:layout_marginTop="@dimen/activity_vertical_margin_half"                    android:text="Stops"/>            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:orientation="vertical"                          android:layout_marginTop="@dimen/activity_vertical_margin_half"            >                <android.support.v7.widget.SwitchCompat                        android:id="@+id/switchButton_direct"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="@dimen/margin_2dp"                        android:layout_marginRight="@dimen/margin_2dp"                        style="@style/SwitchCompatStyle"                        android:checked="true"                        android:text="Direct"                />                <android.support.v7.widget.SwitchCompat                        android:id="@+id/switchButton_1stop"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="@dimen/margin_2dp"                        android:layout_marginRight="@dimen/margin_2dp"                        style="@style/SwitchCompatStyle"                        android:checked="true"                        android:text="1 Stop"                />                <android.support.v7.widget.SwitchCompat                        android:id="@+id/switchButton_2stops"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="@dimen/margin_2dp"                        android:layout_marginRight="@dimen/margin_2dp"                        style="@style/SwitchCompatStyle"                        android:checked="true"                        android:text="2 + Stops"/>            </LinearLayout>            <LinearLayout                    android:id="@+id/ll_apply"                    android:layout_marginTop="@dimen/activity_vertical_margin_half"                    android:layout_width="match_parent"                    android:layout_height="match_parent"            >                <Button                        android:text="@string/cancel"                        android:layout_width="wrap_content"                        style="@style/CustomButton"                        android:layout_marginRight="@dimen/activity_vertical_margin_half"                        android:layout_weight="1"                        android:id="@+id/button_dialog_cancel"/>                <Button                        android:text="@string/apply"                        android:layout_width="wrap_content"                        style="@style/CustomButton"                        android:layout_weight="1"                        android:id="@+id/button_dialog_apply"/>            </LinearLayout>        </LinearLayout>    </ScrollView></LinearLayout>