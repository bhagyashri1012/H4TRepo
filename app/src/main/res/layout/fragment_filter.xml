<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"              xmlns:app="http://schemas.android.com/apk/res-auto"              android:background="@color/white_pure"              android:orientation="vertical"              android:layout_width="match_parent" android:layout_height="match_parent">    <include layout="@layout/common_toolbar"/>    <ScrollView android:layout_width="match_parent"                android:layout_height="wrap_content"                android:nestedScrollingEnabled="false"    >        <LinearLayout android:layout_width="match_parent"                      android:layout_height="wrap_content"                      android:layout_margin="@dimen/activity_vertical_margin"                      android:paddingBottom="100dp"                      android:orientation="vertical">            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:orientation="vertical">                <TextView style="@style/CustomRegularBoldTextView"                          android:text="@string/price_text"/>                <RelativeLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                >                    <TextView                            android:id="@+id/textMin_price"                            style="@style/CustomMediumTextView"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginStart="10dp"                    />                    <TextView                            android:id="@+id/textMax_price"                            style="@style/CustomMediumTextView"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginEnd="10dp"                            android:layout_alignParentRight="true"/>                </RelativeLayout>                <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                        android:id="@+id/range_seekbar_price"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        app:corner_radius="6"                        app:right_thumb_color="@android:color/holo_blue_light"                        app:left_thumb_color="@android:color/holo_blue_light"                        app:bar_color="@color/cool_grey"                        app:bar_highlight_color="@color/colorPrimary"                />            </LinearLayout>            <TextView style="@style/CustomRegularBoldTextView"                      android:text="@string/duration_text"/>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="vertical"                    android:layout_marginTop="@dimen/activity_vertical_margin_half"                    android:layout_marginBottom="@dimen/margin_4dp"                    android:clipChildren="false">                <RelativeLayout android:layout_width="match_parent"                                android:layout_height="wrap_content">                    <TextView                            android:id="@+id/textMin_duration"                            style="@style/CustomMediumTextView"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginStart="10dp"                    />                    <TextView                            android:id="@+id/textMax_duration"                            android:text="Any"                            style="@style/CustomMediumTextView"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginEnd="10dp"                            android:layout_alignParentRight="true"/>                </RelativeLayout>                <com.crystal.crystalrangeseekbar.widgets.CrystalSeekbar                        android:id="@+id/rb_duration"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="@dimen/margin_4dp"                        app:max_value="48"                        app:min_value="8"                        app:corner_radius="6"                        app:right_thumb_color="@android:color/holo_blue_light"                        app:left_thumb_color="@android:color/holo_blue_light"                        app:bar_color="@color/cool_grey"                        app:bar_highlight_color="@color/colorPrimary"                />            </LinearLayout>            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:id="@+id/ll_time"                          android:orientation="vertical"            >                <TextView style="@style/CustomRegularBoldTextView"                          android:text="@string/time"/>                <TextView style="@style/CustomRegularTextView"                          android:layout_marginTop="@dimen/activity_horizontal_margin"                          android:text="@string/outbound_takeoff_time"/>                <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical"                        android:layout_marginTop="@dimen/activity_vertical_margin_half"                        android:layout_marginBottom="@dimen/margin_4dp"                        android:clipChildren="false">                    <RelativeLayout android:layout_width="match_parent"                                    android:layout_height="wrap_content">                        <TextView                                android:id="@+id/textMin_outbound_takeofff"                                style="@style/CustomMediumTextView"                                android:text="00:00"                                android:layout_marginTop="@dimen/margin_4dp"                                android:layout_marginStart="10dp"                        />                        <TextView                                android:id="@+id/textMax_outbound_takeofff"                                android:text="23:59"                                style="@style/CustomMediumTextView"                                android:layout_marginTop="@dimen/margin_4dp"                                android:layout_marginEnd="10dp"                                android:layout_alignParentRight="true"/>                    </RelativeLayout>                    <com.edmodo.rangebar.RangeBar                            android:id="@+id/rb_ob_toff"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginBottom="@dimen/margin_16dp"                            app:corner_radius="6"                    />                </LinearLayout>                <TextView style="@style/CustomRegularTextView"                          android:text="Outbound Landing Time" android:layout_height="wrap_content"/>                <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical"                        android:layout_marginTop="@dimen/activity_vertical_margin_half"                        android:layout_marginBottom="@dimen/margin_4dp"                        android:clipChildren="false">                    <RelativeLayout android:layout_width="match_parent"                                    android:layout_height="wrap_content">                        <TextView                                android:id="@+id/textMin_ob_landing"                                style="@style/CustomMediumTextView"                                android:text="00:00"                                android:layout_marginTop="@dimen/margin_4dp"                                android:layout_marginStart="10dp"                        />                        <TextView                                android:id="@+id/textMax_ob_landing"                                android:text="23:59"                                style="@style/CustomMediumTextView"                                android:layout_marginTop="@dimen/margin_4dp"                                android:layout_marginEnd="10dp"                                android:layout_alignParentRight="true"/>                    </RelativeLayout>                    <com.edmodo.rangebar.RangeBar                            android:id="@+id/rb_ob_landing"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:paddingLeft="@dimen/activity_vertical_margin"                            android:paddingRight="@dimen/activity_vertical_margin"                            android:filterTouchesWhenObscured="true"                            android:layout_marginTop="@dimen/margin_4dp"                            android:layout_marginBottom="@dimen/margin_16dp"                    />                </LinearLayout>                <LinearLayout                        android:id="@+id/ll_inbound"                        android:layout_width="match_parent" android:layout_height="wrap_content"                        android:orientation="vertical"                >                    <TextView style="@style/CustomRegularTextView"                              android:text="@string/inbound_takeoff_time"/>                    <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:layout_marginTop="@dimen/activity_vertical_margin_half"                            android:layout_marginBottom="@dimen/margin_4dp"                            android:clipChildren="false">                        <RelativeLayout android:layout_width="match_parent"                                        android:layout_height="wrap_content">                            <TextView                                    android:id="@+id/textMin_inbound_takeofff"                                    style="@style/CustomMediumTextView"                                    android:text="00:00"                                    android:layout_marginTop="@dimen/margin_4dp"                                    android:layout_marginStart="10dp"                            />                            <TextView                                    android:id="@+id/textMax_inbound_takeofff"                                    android:text="23:59"                                    style="@style/CustomMediumTextView"                                    android:layout_marginTop="@dimen/margin_4dp"                                    android:layout_marginEnd="10dp"                                    android:layout_alignParentRight="true"/>                        </RelativeLayout>                        <com.edmodo.rangebar.RangeBar                                android:id="@+id/rb_in_toff"                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_marginTop="@dimen/margin_4dp"                                android:layout_marginBottom="@dimen/margin_16dp"                                android:paddingLeft="@dimen/activity_vertical_margin"                                android:paddingRight="@dimen/activity_vertical_margin"                        />                    </LinearLayout>                    <TextView style="@style/CustomRegularTextView"                              android:text="Inbound Landing Time" android:layout_height="wrap_content"/>                    <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:layout_marginTop="@dimen/activity_vertical_margin_half"                            android:layout_marginBottom="@dimen/margin_4dp"                            android:clipChildren="false">                        <RelativeLayout android:layout_width="match_parent"                                        android:layout_height="wrap_content">                            <TextView                                    android:id="@+id/textMin_in_landing"                                    style="@style/CustomMediumTextView"                                    android:text="00:00"                                    android:layout_marginTop="@dimen/margin_4dp"                                    android:layout_marginStart="10dp"                            />                            <TextView                                    android:id="@+id/textMax_in_landing"                                    android:text="23:59"                                    style="@style/CustomMediumTextView"                                    android:layout_marginTop="@dimen/margin_4dp"                                    android:layout_marginEnd="10dp"                                    android:layout_alignParentRight="true"/>                        </RelativeLayout>                        <com.edmodo.rangebar.RangeBar                                android:id="@+id/rb_in_landing"                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_marginTop="@dimen/margin_4dp"                                android:layout_marginBottom="@dimen/margin_16dp"                        />                    </LinearLayout>                </LinearLayout>            </LinearLayout>            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:id="@+id/ll_stops"                          android:orientation="vertical"            >                <TextView                        style="@style/CustomRegularBoldTextView"                        android:layout_marginTop="@dimen/activity_vertical_margin_half"                        android:layout_marginBottom="@dimen/activity_vertical_margin_half"                        android:text="Stops"/>                <android.support.v7.widget.SwitchCompat                        android:id="@+id/switchButton_direct"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="@dimen/margin_2dp"                        android:layout_marginRight="@dimen/margin_2dp"                        style="@style/SwitchCompatStyle"                        android:checked="true"                        android:text="Direct"                />                <android.support.v7.widget.SwitchCompat                        android:id="@+id/switchButton_1stop"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="@dimen/margin_2dp"                        android:layout_marginRight="@dimen/margin_2dp"                        style="@style/SwitchCompatStyle"                        android:checked="true"                        android:text="1 Stop"                />                <android.support.v7.widget.SwitchCompat                        android:id="@+id/switchButton_2stops"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginBottom="@dimen/margin_2dp"                        android:layout_marginRight="@dimen/margin_2dp"                        style="@style/SwitchCompatStyle"                        android:checked="true"                        android:text="2 + Stops"/>            </LinearLayout>            <LinearLayout                    android:id="@+id/ll_apply"                    android:layout_marginTop="@dimen/activity_vertical_margin_half"                    android:layout_width="match_parent"                    android:layout_height="match_parent"            >                <Button                        android:text="@string/cancel"                        android:layout_width="wrap_content"                        style="@style/CustomButton"                        android:layout_marginRight="@dimen/activity_vertical_margin_half"                        android:layout_weight="1"                        android:id="@+id/button_dialog_cancel"/>                <Button                        android:text="@string/apply"                        android:layout_width="wrap_content"                        style="@style/CustomButton"                        android:layout_weight="1"                        android:id="@+id/button_dialog_apply"/>            </LinearLayout>        </LinearLayout>    </ScrollView></LinearLayout>