<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"             xmlns:tools="http://schemas.android.com/tools"             xmlns:app="http://schemas.android.com/apk/res-auto"             android:background="@color/white_pure"             android:layout_width="match_parent" android:layout_height="match_parent"             tools:context="com.usit.hub4tickets.flight.ui.FilterFragment">        <ScrollView android:layout_width="match_parent"                    android:layout_height="wrap_content"        >            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:layout_margin="@dimen/activity_vertical_margin"                          android:paddingBottom="100dp"                          android:orientation="vertical">                <TextView style="@style/CustomMediumBoldTextView"                          android:text="@string/price_text"/>                <RelativeLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="@dimen/margin_4dp"                        android:clipChildren="false">                    <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                            android:id="@+id/range_seekbar_price"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            app:right_thumb_color="@color/colorPrimary"                            app:left_thumb_color="@color/colorPrimary"                            app:bar_color="@color/colorPrimary"                            app:bar_highlight_color="@color/colorPrimaryDark"                            app:min_value="1"                    />                    <TextView                            android:id="@+id/textMin_price"                            style="@style/CustomRegularBoldTextView"                            android:text="0"                            android:layout_below="@id/range_seekbar_price"                            android:layout_marginTop="5dp"                            android:layout_marginStart="10dp"                            />                    <TextView                            android:id="@+id/textMax_price"                            style="@style/CustomRegularBoldTextView"                            android:text="0"                            android:layout_below="@id/range_seekbar_price"                            android:layout_marginTop="5dp"                            android:layout_marginEnd="10dp"                            android:layout_alignParentRight="true"/>                </RelativeLayout>                <TextView style="@style/CustomMediumBoldTextView"                          android:text="@string/duration_text"/>                <LinearLayout android:layout_width="match_parent"                              android:layout_height="wrap_content"                              android:orientation="vertical"                              android:layout_marginTop="@dimen/margin_4dp"                >                    <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:clipChildren="false">                        <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                                android:id="@+id/range_seekbar"                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                        app:right_thumb_color="@color/colorPrimary"                                app:left_thumb_color="@color/colorPrimary"                                app:bar_color="@color/colorPrimary"                                app:bar_highlight_color="@color/colorPrimaryDark"                                app:min_value="1"                                app:max_value="10"                        />                        <TextView                                android:id="@+id/textMin"                                style="@style/CustomRegularBoldTextView"                                android:text="1"                                android:layout_below="@id/range_seekbar"                                android:layout_marginTop="5dp"                                android:layout_marginStart="10dp"                                />                        <TextView                                android:id="@+id/textMax"                                android:text="10"                                style="@style/CustomRegularBoldTextView"                                android:layout_below="@id/range_seekbar"                                android:layout_marginTop="5dp"                                android:layout_marginEnd="10dp"                                android:layout_alignParentRight="true"/>                    </RelativeLayout>                    <TextView style="@style/CustomMediumBoldTextView"                              android:text="OutboundTrip"/>                    <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginTop="@dimen/margin_4dp"                            android:clipChildren="false">                        <com.crystal.crystalrangeseekbar.widgets.CrystalRangeSeekbar                                android:id="@+id/range_seekbar_outbound_trip"                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                app:right_thumb_color="@color/colorPrimary"                                app:left_thumb_color="@color/colorPrimary"                                app:bar_color="@color/colorPrimary"                                app:bar_highlight_color="@color/colorPrimaryDark"                                app:min_value="1"                                app:max_value="10"                        />                        <TextView                                android:id="@+id/textMin_outbound"                                style="@style/CustomRegularBoldTextView"                                android:text="1"                                android:layout_below="@id/range_seekbar_outbound_trip"                                android:layout_marginTop="5dp"                                android:layout_marginStart="10dp"                                />                        <TextView                                android:id="@+id/textMax_outbound"                                android:text="10"                                style="@style/CustomRegularBoldTextView"                                android:layout_below="@id/range_seekbar_outbound_trip"                                android:layout_marginTop="5dp"                                android:layout_marginEnd="10dp"                                android:layout_alignParentRight="true"/>                    </RelativeLayout>                </LinearLayout>                <TextView                        style="@style/CustomMediumBoldTextView"                        android:layout_marginTop="@dimen/activity_vertical_margin_half"                        android:text="Stops"/>                <LinearLayout android:layout_width="match_parent"                              android:layout_height="wrap_content"                              android:orientation="vertical"                              android:layout_marginTop="@dimen/activity_vertical_margin_half"                >                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_direct"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="Direct"                    />                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_1stop"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="1 Stop"                    />                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_2stops"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="2 Stops"/>                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_over_night_stop"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="Overnight Stop"/>                </LinearLayout>                <LinearLayout                        android:id="@+id/ll_apply"                        android:layout_marginTop="@dimen/activity_vertical_margin"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                >                    <Button                            android:text="@string/cancel"                            android:layout_width="wrap_content"                            style="@style/CustomButton"                            android:layout_marginRight="@dimen/activity_vertical_margin_half"                            android:layout_weight="1"                            android:id="@+id/button_dialog_cancel"/>                    <Button                            android:text="@string/apply"                            android:layout_width="wrap_content"                            style="@style/CustomButton"                            android:layout_weight="1"                            android:id="@+id/button_dialog_apply"/>                </LinearLayout>            </LinearLayout>        </ScrollView></FrameLayout>