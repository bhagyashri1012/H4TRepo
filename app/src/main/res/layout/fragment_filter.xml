<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"             xmlns:tools="http://schemas.android.com/tools"             xmlns:app="http://schemas.android.com/apk/res-auto"             android:layout_width="match_parent" android:layout_height="match_parent"             tools:context="com.usit.hub4tickets.flight.ui.FilterFragment">    <LinearLayout android:layout_width="match_parent"                  android:layout_height="wrap_content"                  android:orientation="vertical">        <include layout="@layout/common_toolbar"/>        <ScrollView android:layout_width="match_parent"                    android:layout_height="wrap_content">            <LinearLayout android:layout_width="match_parent"                          android:layout_height="wrap_content"                          android:layout_margin="@dimen/activity_vertical_margin"                          android:orientation="vertical">                <TextView style="@style/CustomMediumBoldTextView"                          android:text="@string/price_text"/>                <SeekBar android:layout_width="match_parent"                         android:layout_height="wrap_content"                         android:layout_alignParentLeft="true"                         android:layout_alignParentTop="true"                         android:layout_marginTop="@dimen/activity_vertical_margin_half"                         android:layout_marginBottom="@dimen/activity_vertical_margin_half"                         android:max="10"                />                <TextView style="@style/CustomMediumBoldTextView"                          android:text="@string/duration_text"/>                <LinearLayout android:layout_width="match_parent"                              android:layout_height="wrap_content"                              android:orientation="vertical"                              android:layout_marginRight="@dimen/activity_vertical_margin_half"                              android:layout_marginLeft="@dimen/activity_vertical_margin_half"                              android:layout_marginTop="@dimen/activity_vertical_margin_half"                >                    <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginTop="20dp"                            android:clipChildren="false">                        <com.crystal.crystalrangeseekbar.widgets.CrystalSeekbar                                android:id="@+id/rangeSeekbar1"                                android:layout_width="match_parent"                                android:layout_height="wrap_content"/>                        <TextView                                android:id="@+id/textMin1"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="0"                                android:layout_below="@id/rangeSeekbar1"                                android:layout_marginTop="5dp"                                android:layout_marginStart="10dp"                                android:textSize="16dp"                                android:textColor="#000"/>                        <TextView                                android:id="@+id/textMax1"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="100"                                android:layout_below="@id/rangeSeekbar1"                                android:layout_marginTop="5dp"                                android:layout_marginEnd="10dp"                                android:textSize="16dp"                                android:textColor="#000"                                android:layout_alignParentRight="true"/>                    </RelativeLayout>                    <TextView style="@style/CustomRegularTextView"                              android:text="OutboundTrip"/>                    <SeekBar android:layout_width="match_parent"                             android:layout_height="wrap_content"                             android:layout_alignParentLeft="true"                             android:layout_alignParentTop="true"                             android:layout_marginTop="@dimen/activity_vertical_margin_half"                             android:layout_marginBottom="@dimen/activity_vertical_margin_half"                             android:max="10"                    />                </LinearLayout>                <TextView                        style="@style/CustomMediumBoldTextView"                        android:text="Stops"/>                <LinearLayout android:layout_width="match_parent"                              android:layout_height="wrap_content"                              android:orientation="vertical"                              android:layout_marginRight="@dimen/activity_vertical_margin_half"                              android:layout_marginLeft="@dimen/activity_vertical_margin_half"                              android:layout_marginTop="@dimen/activity_vertical_margin_half"                >                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_direct"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="Direct"                    />                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_1stop"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="1 Stop"                    />                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_2stops"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="2 Stops"/>                    <android.support.v7.widget.SwitchCompat                            android:id="@+id/switchButton_over_night_stop"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            style="@style/CustomRegularTextView"                            app:trackTint="@color/colorPrimary"                            app:thumbTint="@color/white_pure"                            android:checked="true"                            android:text="Overnight Stop"/>                </LinearLayout>                <LinearLayout                        android:id="@+id/ll_apply"                        android:layout_marginTop="@dimen/margin_32"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                >                    <Button                            android:text="@string/cancel"                            android:layout_width="wrap_content"                            style="@style/CustomButton"                            android:layout_marginRight="@dimen/activity_vertical_margin_half"                            android:layout_weight="1"                            android:id="@+id/button_dialog_cancel"/>                    <Button                            android:text="@string/apply"                            android:layout_width="wrap_content"                            style="@style/CustomButton"                            android:layout_weight="1"                            android:id="@+id/button_dialog_apply"/>                </LinearLayout>            </LinearLayout>        </ScrollView>    </LinearLayout></FrameLayout>