<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"             xmlns:tools="http://schemas.android.com/tools"             android:id="@+id/container_account_info"             android:layout_width="match_parent"             android:layout_height="match_parent"             android:background="@color/white_pure"             tools:context="com.usit.hub4tickets.dashboard.ui.MyAccountFragment">    <ScrollView            xmlns:android="http://schemas.android.com/apk/res/android"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:fillViewport="true"            android:fadingEdge="none"            android:scrollbars="none"            android:overScrollMode="never"            android:layout_marginBottom="50dp"    >        <RelativeLayout android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical"        >            <LinearLayout android:layout_width="match_parent"                          android:layout_height="match_parent"                          android:orientation="vertical"                          android:weightSum="3">                <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                >                    <LinearLayout android:layout_width="match_parent"                                  android:layout_height="wrap_content"                                  android:orientation="vertical"                                  android:layout_margin="@dimen/activity_vertical_margin">                        <TextView                                android:id="@+id/link_login"                                style="@style/CustomMediumTextView"                                tools:text="@string/login"                                android:padding="@dimen/margin_4dp"                                android:layout_marginBottom="@dimen/padding_8dp"                        />                        <LinearLayout android:id="@+id/rl_my_acc_info"                                      android:layout_width="match_parent"                                      android:layout_height="wrap_content"                                      android:orientation="horizontal"                                      android:weightSum="2"                        >                            <TextView                                    android:id="@+id/link_account_info"                                    style="@style/CustomRegularTextView"                                    android:layout_weight="1.5"                                    android:text="@string/my_account_info"                                    android:padding="@dimen/margin_4dp"                                    android:layout_marginRight="@dimen/margin_4dp"                                    android:textColor="@color/colorPrimary"                            />                            <TextView                                    android:id="@+id/link_log_out"                                    style="@style/CustomMediumTextView"                                    android:layout_weight=".5"                                    android:text="@string/logout"                                    android:layout_alignParentRight="true"                                    android:padding="@dimen/margin_4dp"                            />                        </LinearLayout>                    </LinearLayout>                </LinearLayout>                <View                        style="@style/Divider"                />                <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                >                    <LinearLayout android:layout_width="match_parent"                                  android:layout_height="wrap_content"                                  android:orientation="vertical"                                  android:layout_margin="@dimen/activity_vertical_margin">                        <TextView                                style="@style/CustomMediumTextView"                                android:text="Settings"                                android:layout_marginBottom="@dimen/activity_vertical_margin"/>                        <LinearLayout android:layout_width="match_parent"                                      android:layout_height="wrap_content"                                      android:orientation="vertical"                        >                            <TextView                                    android:id="@+id/tv_country"                                    android:layout_weight="1"                                    style="@style/CustomRegularTextView"                                    android:text="@string/country_region"                                    android:textColor="@color/slate_grey"                                    android:layout_marginRight="@dimen/activity_vertical_margin_half"                                    android:layout_marginBottom="@dimen/activity_vertical_margin_half"/>                            <TextView                                    android:id="@+id/tv_country_name"                                    android:layout_weight="1"                                    android:maxLines="2"                                    style="@style/CustomRegularTextView"                                    android:textColor="@color/slate_grey"                                    tools:text="India"                                    android:layout_toRightOf="@+id/tv_country"                                    android:layout_marginBottom="@dimen/activity_vertical_margin"/>                        </LinearLayout>                        <LinearLayout android:layout_width="match_parent"                                      android:layout_height="wrap_content"                                      android:orientation="vertical">                            <TextView                                    android:id="@+id/tv_lang"                                    style="@style/CustomRegularTextView"                                    android:layout_weight="1"                                    android:text="@string/language_settings"                                    android:textColor="@color/slate_grey"                                    android:layout_marginRight="@dimen/activity_vertical_margin_half"                                    android:layout_marginBottom="@dimen/activity_vertical_margin_half"/>                            <TextView                                    android:id="@+id/tv_lang_name"                                    android:layout_weight="1"                                    style="@style/CustomRegularTextView"                                    android:textColor="@color/slate_grey"                                    tools:text="Marathi"                                    android:maxLines="2"                                    android:layout_toRightOf="@+id/tv_lang"                                    android:layout_marginBottom="@dimen/activity_vertical_margin"/>                        </LinearLayout>                        <LinearLayout android:layout_width="match_parent"                                      android:layout_height="wrap_content"                                      android:orientation="vertical"                        >                            <TextView                                    android:id="@+id/tv_currency"                                    android:layout_weight="1"                                    style="@style/CustomRegularTextView"                                    android:text="Currency "                                    android:textColor="@color/slate_grey"                                    android:layout_marginRight="@dimen/activity_vertical_margin_half"                                    android:layout_marginBottom="@dimen/activity_vertical_margin_half"/>                            <TextView                                    android:id="@+id/tv_currency_name"                                    android:layout_weight="1"                                    android:maxLines="2"                                    style="@style/CustomRegularTextView"                                    android:textColor="@color/slate_grey"                                    tools:text="INRsadasdasdasdadsasdadadad"                                    android:layout_toRightOf="@+id/tv_currency"                            />                        </LinearLayout>                    </LinearLayout>                </LinearLayout>                <View                        style="@style/Divider"                />                <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                >                    <LinearLayout android:layout_width="match_parent"                                  android:layout_height="wrap_content"                                  android:orientation="vertical"                    >                        <TextView                                android:id="@+id/link_my_alerts"                                style="@style/CustomMediumTextView"                                android:text="@string/my_alerts"                                android:enabled="false"                                android:layout_margin="@dimen/margin_16dp"                        />                        <View style="@style/Divider"/>                        <TextView                                android:id="@+id/link_my_history"                                style="@style/CustomMediumTextView"                                android:text="@string/my_history"                                android:enabled="false"                                android:layout_margin="@dimen/activity_vertical_margin"/>                        <View                                style="@style/Divider"                        />                        <TextView                                android:id="@+id/link_clear_history"                                style="@style/CustomMediumTextView"                                android:text="@string/clear_history"                                android:enabled="false"                                android:layout_margin="@dimen/activity_vertical_margin"/>                        <View                                style="@style/Divider"                        />                    </LinearLayout>                </LinearLayout>            </LinearLayout>        </RelativeLayout>    </ScrollView></FrameLayout>