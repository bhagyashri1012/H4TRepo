<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"              android:orientation="vertical"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_marginLeft="@dimen/margin_16dp"              android:layout_marginRight="@dimen/margin_16dp"              android:layout_marginTop="@dimen/padding_8dp">    <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:weightSum="1"    >        <LinearLayout                android:id="@+id/ll2_first"                android:layout_weight=".11"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_marginRight="@dimen/margin_12dp"                android:layout_marginBottom="@dimen/margin_2dp"                android:orientation="horizontal">            <ImageView                    android:id="@+id/imv_out_bound"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/imv_width"                    android:adjustViewBounds="true"                    android:src="@mipmap/ic_launcher"/>        </LinearLayout>        <View android:layout_width="1dp"              android:layout_height="35dp"              android:background="@color/cool_grey"              android:layout_marginRight="@dimen/activity_vertical_margin_half"        />        <LinearLayout                android:layout_weight=".59"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_marginBottom="@dimen/margin_2dp"                android:layout_marginLeft="@dimen/margin_4dp"                android:layout_marginRight="@dimen/margin_4dp"                android:layout_toRightOf="@+id/ll2_first"                android:orientation="vertical"        >            <TextView                    android:id="@+id/tv_out_bound_time"                    style="@style/CustomRegularTextView"                    tools:text="2.14am - 4.25am"                    android:layout_marginBottom="@dimen/margin_4dp"                    android:maxLines="2"            />            <TextView                    android:id="@+id/tv_out_bound_destination"                    style="@style/CustomSmallTextView"                    tools:text="BOM-PNQ ,GoAir"                    android:textColor="@color/cool_grey"                    android:maxLines="2"            />        </LinearLayout>        <LinearLayout                android:layout_weight=".3"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_marginBottom="@dimen/margin_2dp"                android:layout_toRightOf="@+id/ll_second"                android:layout_alignParentRight="true"                android:orientation="vertical"        >            <TextView                    android:id="@+id/tv_out_bound_duration"                    style="@style/CustomRegularTextView"                    android:gravity="end"                    android:layout_width="match_parent"                    android:textAlignment="viewEnd"                    android:text="2.hr 5min"                    android:maxLines="2"                    android:layout_marginBottom="@dimen/margin_4dp"            />            <TextView                    android:id="@+id/stop_count_outbound"                    tools:text="1 stop"                    android:layout_gravity="end|bottom"                    android:maxLines="1"                    android:gravity="end|right"                    android:textAlignment="viewEnd"                    android:layout_weight="1"                    android:textColor="@color/rose"                    android:layout_marginBottom="@dimen/margin_2dp"                    style="@style/CustomRegularTextView"/>        </LinearLayout>    </LinearLayout>    <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginBottom="@dimen/activity_vertical_margin_half"            android:orientation="horizontal"            android:weightSum="1"    >        <LinearLayout                android:id="@+id/ll_first"                android:layout_weight=".1"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_marginRight="@dimen/margin_12dp"                android:orientation="horizontal">            <ImageView                    android:id="@+id/imv_inbound"                    android:layout_width="wrap_content"                    android:layout_height="@dimen/imv_width"                    android:adjustViewBounds="true"                    android:src="@mipmap/ic_launcher"/>        </LinearLayout>        <View                android:id="@+id/divider"                android:layout_width="1dp"              android:layout_height="35dp"              android:background="@color/warm_grey"              android:layout_marginRight="@dimen/activity_vertical_margin_half"        />        <LinearLayout                android:id="@+id/ll_second"                android:layout_weight=".58"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_marginRight="@dimen/margin_4dp"                android:layout_marginLeft="@dimen/margin_4dp"                android:layout_toRightOf="@+id/ll_first"                android:orientation="vertical"        >            <TextView                    android:id="@+id/tv_time"                    style="@style/CustomRegularTextView"                    tools:text="2.14am - 4.25am"                    android:layout_marginBottom="@dimen/margin_4dp"                    android:maxLines="2"            />            <TextView                    android:id="@+id/tv_destination"                    style="@style/CustomSmallTextView"                    tools:text="BOM-PNQ ,GoAir"                    android:textColor="@color/cool_grey"                    android:maxLines="2"            />        </LinearLayout>        <LinearLayout                android:layout_weight=".3"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_toRightOf="@+id/ll_second"                android:layout_alignParentRight="true"                android:orientation="vertical"        >            <TextView                    android:id="@+id/tv_duration"                    style="@style/CustomRegularTextView"                    android:gravity="end"                    android:layout_width="match_parent"                    android:textAlignment="viewEnd"                    tools:text="2.hr 5min"                    android:maxLines="2"                    android:layout_marginBottom="@dimen/margin_4dp"            />            <TextView                    android:id="@+id/stop_count"                    tools:text="1 stop"                    android:layout_gravity="end|bottom"                    android:maxLines="1"                    android:gravity="end|right"                    android:textAlignment="viewEnd"                    android:layout_weight="1"                    android:textColor="@color/rose"                    android:layout_marginBottom="@dimen/margin_2dp"                    style="@style/CustomRegularTextView"/>        </LinearLayout>    </LinearLayout>    <TextView            android:id="@+id/list_item"            tools:text="$8,000 $"            android:layout_gravity="end"            style="@style/CustomRegularTextView"/>    <View            android:id="@+id/separator"            android:layout_width="match_parent"            android:layout_height="0.5dp"            android:layout_marginTop="@dimen/activity_vertical_margin_half"            android:background="@color/white_grey"/></LinearLayout>